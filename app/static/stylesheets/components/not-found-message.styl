.u-not-found {
  color c-gray
  padding 30px 0px
  text-align center
  overflow hidden

  visibility hidden
  opacity 0
  user-select none
  transform scale(0)

  transition t-default

  h5 {
    font-size 20px
    strong {
      color c-orange
      &:before{
        content ' '
      }
    }
  }
  h6 {
    font-size 10px
    text-transform uppercase
  }

  &.is-active {
    visibility visible
    opacity 1
    user-select inherit
    transform scale(1)
  }
}
